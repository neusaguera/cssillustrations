  height: 115px;
  width: 190px;
  background: #44647A;
  border-radius: 50%;
  -webkit-border-radius: 50%;
  -moz-border-radius: 50%;
  -ms-border-radius: 50%;
  -o-border-radius: 50%; }
  .lab-explosion:before, .lab-explosion:after, .lab-explosion-bubble:before, .lab-explosion-bubble:after {
    content: '';
    position: absolute;
    left: -3px;
    top: -41px;
    height: 81px;
    width: 95px;
    background: #44647A;
    border-radius: 50%;
    -webkit-border-radius: 50%;
    -moz-border-radius: 50%;
    -ms-border-radius: 50%;
    -o-border-radius: 50%; }
  .lab-explosion:after, .lab-explosion-bubble:after {
    top: 60px;
    left: 11px; }

.lab-explosion-bubble {
  top: 16px;
  left: 0; }
  .lab-explosion-bubble:before, .lab-explosion-bubble:after {
    left: 83px;
    top: -41px; }
  .lab-explosion-bubble:after {
    top: 60px;
    right: 11px; }

@keyframes shake {
  1%, 30% {
    transform: translate3d(-50%, 0, 0%);
