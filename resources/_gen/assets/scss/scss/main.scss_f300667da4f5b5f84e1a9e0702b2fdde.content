.lab-fire {
  height: 75px;
  width: 73px;
  background: #EBF2FF;
  position: absolute;
  bottom: 7px;
  border-radius: 5px 50px 50px 50px;
  transform: rotate(45deg);
  left: 22%;
  border-left: 2px solid #44647A;
  border-top: 1px solid #44647A;
  animation: fire-color 10s linear 1;
  -webkit-animation: fire-color 10s linear 1; }

.lab-fire-flame {
  height: 48px;
  width: 48px;
  background: #ED254E;
  position: absolute;
  bottom: 10px;
  border-radius: 5px 50px 50px 50px;
