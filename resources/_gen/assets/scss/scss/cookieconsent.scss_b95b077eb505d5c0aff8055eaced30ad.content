.wpcc-transparency-10{opacity:.9}.wpcc-container{border-style:solid;border-width:0;box-sizing:border-box;display:flex;flex-wrap:nowrap;font-family:inherit;font-size:16px;line-height:1.5em;overflow:hidden;position:fixed;transition:opacity 1s ease;z-index:9999}@media print{.wpcc-container{display:none}}.wpcc-pushdown-wrap{max-height:0;overflow:hidden;transition:max-height 1s}.wpcc-pushdown{margin:0;position:static}.wpcc-invisible{opacity:0}.wpcc-banner{flex-direction:row;margin:1em;padding:1em;align-items:center}.wpcc-banner.wpcc-margin-none{margin:0}.wpcc-btn,.wpcc-privacy{cursor:pointer}.wpcc-privacy{display:inline-block;opacity:.85;text-decoration:underline}.wpcc-privacy:hover{opacity:1}.wpcc-privacy:active,.wpcc-privacy:visited{color:initial}.wpcc-btn{display:block;font-size:.9em;font-weight:700;padding:.4em .8em;text-align:center;white-space:nowrap;transition:background-color .2 ease}.wpcc-btn:hover{text-decoration:none}.wpcc-bottom{bottom:1em}.wpcc-bottom.wpcc-margin-none{bottom:0}.wpcc-banner.wpcc-bottom{left:0;right:0;bottom:0}.wpcc-banner .wpcc-message{display:block;flex:1 1 auto;margin-right:1em;max-width:100%}.wpcc-float .wpcc-compliance{flex:1 0 auto}@media screen and (max-width:414px) and (orientation:portrait),screen and (max-width:736px) and (orientation:landscape){.wpcc-bottom{bottom:0}.wpcc-banner{align-items:unset;flex-direction:column}.wpcc-message{margin-bottom:1em}}